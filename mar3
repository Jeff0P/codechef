// rahul2240

#include <iostream>
using namespace std;

int main() {
	long long int t,n,a[100000],i,j,max,count;
	
	cin>>t;
	
	while(t--)
	{
	    cin>>n;
	    
	    for(i=0;i<n;i++)
	    {
	        cin>>a[i];
	        
	        if(i==0)
	            max = a[i];
	        else
	        {
	            if(max<a[i])
	            {
	                max = a[i];
	            }
	        }
	    }
	        count=0;
	        for(i=0;i<n-1;i++)
	        {
	            for(j=i+1;j<n;j++)
	            {
	                if(a[i]==a[j])
	                {
	                    a[j] = a[j]+max;
	                    max = a[j];
	                    count+=1;
	                }
	            }
	        }
	        
	        cout<<count<<endl;
	        
	  
	    
	}
	return 0;
}
