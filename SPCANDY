#include <iostream>
using namespace std;

int main() {
  
  long long int t,n,k;
  
  cin>>t;
  
  while(t--)
  {
    cin>>n>>k;
    
    if(n==0)
    {
      cout<<"0 0"<<endl;
      
    }
    else if(k==0)
    {
      cout<<"0 "<<n<<endl;
    }
    else
    {
      cout<<((n-n%k)/k)<<" "<<n%k<<endl;
    }
  }
  
  return 0;
}
