#include <iostream>

using namespace std;

int main()
{
    long long int t, n, k, i, num, sum;

    cin>>t;

    while(t--)
    {
        cin>>n>>k;
        sum=0;
        for(i=0;i<n;i++)
        {
            cin>>num;
            sum = sum + num;
        }

        if(k==1){
            if(sum%2==0)
            cout<<"odd"<<endl;
            else
                cout<<"even"<<endl;
        }
        else
            cout<<"even"<<endl;
    }
    return 0;
}
