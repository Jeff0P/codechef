#include <iostream>
using namespace std;

int main() {
  int t,x1,x2,y1,y2;
  
  cin>>t;
  
  while(t--)
  {
    cin>>x1>>y1>>x2>>y2;
    
    if(x1==x2)
    {
      if (y1>y2)
      {
        cout<<"Down"<<endl;
      }
      else
        cout<<"Up"<<endl;
    }
    else if(y1==y2)
    {
        if (x1>x2)
      {
        cout<<"Left"<<endl;
      }
      else
        cout<<"Right"<<endl;
      
    }
    else
    {
      cout<<"Impossible"<<endl;
    }
    
    
  }
}
